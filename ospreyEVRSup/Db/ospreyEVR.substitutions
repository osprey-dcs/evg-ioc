#   Undefined macros
#
# P - Common record name prefix
# NAME - Device instance name

file "perEVR.template" {
pattern { P,  NAME }
  { "\$(P)", "\$(NAME)" }
}

file "perEVRhwTrigger.template" {
pattern { P,     NAME   ,  I }
  { "\$(P)", "\$(NAME)", "1" }
  { "\$(P)", "\$(NAME)", "2" }
  { "\$(P)", "\$(NAME)", "3" }
  { "\$(P)", "\$(NAME)", "4" }
}

